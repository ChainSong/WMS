@model Runbow.TWS.Web.Areas.POD.Models.OrderNoInfoUpdate

@Html.Partial("MsgMask")
@using (Html.BeginForm("OrderNoInfoUpdate", "Hilti", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
     <fieldset>
    <legend>请选择Excel
    </legend>
    <div id="importDiv">
         <label for="file">Upload File:</label>
         <input type="file" name="fileToUpload" id="importExcel"  class="fileImport" />
         <input type="submit" value="操作更新" class="fileImport" onclick="return fileImportClick();"/>
    </div>
    </fieldset>
    
    <fieldset>
    <legend>更新结果
    </legend>
    <div>
        @if(Model.Message!=null)
        {
            @Html.Raw(Model.Message);
        }
    </div>
</fieldset>
}

