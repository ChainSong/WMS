                            <label>高（cm）</label>
                        </td>
                        <td style="width: 20%;">@Html.TextBox("Height", null, new { @class = "form-control", style = "width:100%", @readonly = "true" })</td>
                    </tr>
                    <tr>
                        <td class="TableColumnTitle">
                            <label>箱号</label>
                        </td>
                        <td style="width: 20%;">@Html.TextBox("PackageNumber", FirstBoxnumber, new { @class = "form-control", style = "width:100%", @readonly = "true" })</td>
                        <td class="TableColumnTitle">
                            <label>体积（㎡）</label>
                        </td>
                        <td style="width: 20%;">@Html.TextBox("NetWeight", null, new { @class = "form-control", style = "width:100%", @readonly = "true" })</td>
                        <td class="TableColumnTitle">
                            <label>毛重（㎏）</label>
                        </td>
                        <td style="width: 20%;">@Html.TextBox("GrossWeight", null, new { @class = "form-control", style = "width:100%" })</td>
                    </tr>
                </table>
                <table id="resultTable">
                    <thead>
                        <tr>
                            <th>
                                SKU
                            </th>
                            <th>
                                UPC
                            </th>
                            <th>
                                货品等级
                            </th>
                            <th>
                                货品描述
                            </th>
                            <th>
                                数量
                            </th>
                            <th>
                                操作
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><input type="text" class="form-control" style="width: 100%" id="SKU" readonly="readonly" /></td>
                            <td><input type="text" class="form-control" style="width: 100%" id="UPC" readonly="readonly" /></td>
                            <td><input type="text" class="form-control" style="width: 100%" id="GoodsType" readonly="readonly" /></td>
                            <td><input type="text" class="form-control" style="width: 100%" id="GoodsName" readonly="readonly" /></td>
                            <td><input type="text" class="form-control" style="width: 100%" id="Qty" readonly="readonly" onchange="QtyChanged(this)" /></td>
                            <td style="background-color: white;">@*<label  style="cursor:pointer; color:white;font-size:15px" class="label label-info" onclick="addNew(this)">新增行</label>*@<label style="cursor: pointer; color: white; font-size: 15px" class="label label-info" onclick="deleteNew(this)">删除行</label></td>
                        </tr>
                    </tbody>
                </table>
            </div>

        }
    </fieldset>

    <div id="showdata2" style="position: fixed;overflow-y:scroll; top: 100px; left: 100px; width: 500px; height: 400px; display: none; background-color: white; border: solid 1px #add9c0;">
        <table id="showtable2"></table>
        <input type="button" value="关闭" onclick="    CloseDiv()" />
    </div>
    <div id="showdata" style="position: fixed; top: 0px; right: 100px; width: 350px; max-height: 150px; background-color: gray; border: solid 1px #add9c0; color: white;">
        @foreach (var item in @Model.OrderDetailCollection)
        {
            var qty = Convert.ToInt32(item.Qty);
            totalcount = totalcount + qty;
        }
        @foreach (var item in @Model.PackageDetailCollection)
        {
            var qty = Convert.ToInt32(totalpackagecount) + Convert.ToInt32(item.Qty);
            totalpackagecount = qty.ToString();
        }
        <table id="showtable">
            @{
                var pickqty = totalcount - Convert.ToInt32(totalpackagecount);
            }

            <tr>
                <td id="totalcount">订单总数:@totalcount</td>
                <td id="packagecount">已完成数量:@totalpackagecount</td>
                <td id="lastcount">剩余数量:@pickqty</td>
                <td>搜索箱号</td>
            </tr>
            <tr>
                <td id="msgtd"></td>
                <td></td>
                <td>
                    <input type="button" value="检测差异" onclick="CheckDiffent()" />
                </td>
                <td>
                    <input type="text" id="SearchBoxID" style="width:100px" onkeydown="SearchBox()" />
                </td>
            </tr>
        </table>
    </div>
    <div id="toExcel" style="position: fixed; top: 100px; left: 100px; width: 300px; height: 400px; display: none; background-color: white; border: solid 1px #add9c0;">
        <table id="toExcelT">
            <tr>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
        </table>
    </div>
}
