<!-- 新 Bootstrap 核心 CSS 文件 -->
<link rel="stylesheet" href="~/Assets/bootstrap-3.3.4-dist/bootstrap.min.css">
<script src="~/Assets/JS/bootstrap-3.3.4-dist/bootstrap3.0.3.min.js"></script>
<script src="~/Scripts/System/Layer/layer.js"></script>
@Url.FrameworkJs("ajaxfileupload.js")
@Html.Partial("MsgMask")
@Url.PopupJs()
@using Runbow.TWS.Common;
@model Runbow.TWS.Web.Areas.WMS.Models.WorkOrderManagement.IndexViewModel
@{}
@using (Html.BeginForm())
{
    <fieldset>
        <legend>预入库单查询条件</legend>
        @*<font color="#0000FF"><marquee direction="left" style="background:#de6e35;font-size:40px">当前有100单待处理</marquee></font>*@
        <table>
            <tr>
                <td class="TableColumnTitle">
                    <label>工单号</label>
                </td>
                <td style="width: 15%;">@Html.TextAreaFor(m => m.WorkOrderCondition.WorkOrderNumber, new { @class = "form-control" })</td>

                <td class="TableColumnTitle">
                    <label>创建日期</label>
                </td>
                <td style="width: 15%;">@Html.CalendarRange("CreateTime", "calendarRange form-control calendarRangeReWrite", @Model.WorkOrderCondition.StartCreateTime, @Model.WorkOrderCondition.EndCreateTime)</td>

            </tr>

        </table>
        <div class="actionButtonDiv">


            <input type="submit" class="btn btn-success" value="查询" id="searchButton" name="Action" />

            <input type="button" class="btn btn-success" value="新增" id="addButton" onclick="createasn()" name="Action" />
        </div>
    </fieldset>
}