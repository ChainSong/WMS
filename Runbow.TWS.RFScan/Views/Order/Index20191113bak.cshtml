    //验证数据是否是SKU
    function checkSKU(sku) {
        recidArray.length = 0;
        var result = false;
        if (objtotal.length > 0) {
            $.each(objtotal, function (index, item) {
                //循环获取数据

                if ((item[2] == sku || item[3] == sku) && item[9]==NowLocation) {
                    if (recidArray.length <= 0) {
                        recidArray[0] = item[0];
                    }
                    else {
                        var index = recidArray.length;
                        recidArray[index] = item[0];
                    }
                    result = true;

                }
            });
        }
        return result;
    }
