        //验证数据是否是库位
        function checkLocation(location) {
            var result = false;
            if (objtotal.length > 0) {
                $.each(objtotal, function (index, item) {

                    //循环获取数据
                    if (item[9] == location) {
                        result = true;
                    }
                });
            }
            return result;
        }

        //验证数据是否是箱号
        function checkBoxNumber(boxnumber) {
            var result = false;
            if (objtotal.length > 0) {
                $.each(objtotal, function (index, item) {
                    //循环获取数据
                    if (item[13] == boxnumber) {
                        result = true;
                    }
                });
            }
            return result;
        }
        //验证数据是否是SKU
        function checkSKU(sku) {
            recidArray.length = 0;
            var result = false;
            if (objtotal.length > 0) {
                $.each(objtotal, function (index, item) {
                    //循环获取数据

                    if ((item[2] == sku || item[3] == sku)) {
                        if (recidArray.length <= 0) {
                            recidArray[0] = item[0];
                        }
                        else {
                            var index = recidArray.length;
                            recidArray[index] = item[0];
                        }
                        result = true;

                    }
                });
            }
            return result;
        }
        //验证是否需要扫SN
        function checkSN(sku) {
            recidArray.length = 0;
            var result = false;
            if (objtotal.length > 0) {
                $.each(objtotal, function (index, item) {
                    //循环获取数据

                    if (item[2] == sku && item[11] == 1) {
                        result = true;

                    }
                });
            }
            return result;
        }
