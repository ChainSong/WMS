        //扫快递单号
        function ScanExpress(event) {
            if (event.keyCode == 13) {
                if (OrderIsCancel()) {
                    layer.alert('订单已取消，请退出操作', {
                        skin: 'layui-layer-lan' //样式类名
                        , icon: 2
                        , closeBtn: 0
                        , btn: ['确定'] //单击按钮
                        , btn1: function (index, layero) {
                            layer.close(index);
                            $("#expressnum").focus();
                            $("#expressnum").select();
                        },
                        success: function (layero) {
                            var btn = layero[0].getElementsByClassName('layui-layer-btn')[0].getElementsByTagName('A')[0];
                            btn.href = 'javascript:void(0)';
                            btn.focus();
                        }
                    })

                }
                else {
                    $.ajax({
                        type: "Post",
                        url: '/Order/CheckOrderExpress',
                        data: { "OrderNumber": $("#ordernum").val(), "ExpressNumber": $("#expressnum").val() },
                        async: false,
                        dataType: "text",
                        success: function (data) {
                            if (data == "1") {
                                $("#SuccessAudio")[0].play();
                                $("#boxnum").focus();
                                $("#boxnum").select();
                            }
                            else {
                                $("#Audio")[0].play();
                                layer.alert('快递单号不正确', {
                                    skin: 'layui-layer-lan' //样式类名
                                    , icon: 2
                                    , closeBtn: 0
                                    , btn: ['确定'] //单击按钮
                                    , btn1: function (index, layero) {
                                        layer.close(index);
                                        $("#expressnum").focus();
                                        $("#expressnum").select();
                                    },
                                    success: function (layero) {
                                        var btn = layero[0].getElementsByClassName('layui-layer-btn')[0].getElementsByTagName('A')[0];
                                        btn.href = 'javascript:void(0)';
                                        btn.focus();
                                    }
                                })
                            }

                        },
                        error: function (msg) {
                            alert("网络连接失败！");
                        }

                    });
                }
            }
        }

