                    //$("#lot2").val("");
                    $("#unit").val("");
                    $("#uom").val("");
                    CloseDiv();
                    $("#receiptnum").focus();

                }
                else {
                    alert("保存失败！");
                }

            },
            error: function (msg) {
                alert("保存失败！");
            }

        });
    }
