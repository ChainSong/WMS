    function getboxcountstring(obj) {
        var res = "";
        if (obj.toString().length == 1) {
            res = "00" + obj.toString();
        }
        else if (obj.toString().length == 2) {
            res = "0" + obj.toString();
        }
        else if (obj.toString().length == 3) {
            res = "0" + obj.toString();
        }
        else {
            res = "001";
        }
        return res;
    }
