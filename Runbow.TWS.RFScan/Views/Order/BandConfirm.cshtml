            });
            jsonString = jsonString.substring(0, jsonString.length - 1);
            jsonString += "]";

            $.ajax({
                type: "Post",
                url: "/Order/UpdatePackageExpress",
                data: {
                    "ID": OrderID,
                    "JsonPackage": jsonString

                },
                async: "false",
                success: function (data) {
                    if (data == "") {
                        alert("提交包装完成！");
                       
                        $("#boxtype").val("");
                        $("#boxnumber").val("");
                        $("#Scan").empty();
                    
                     
                        $("#Scan").val("");
                        $("#ordernum").val("")
                        $("#expressnum").val("")
                        //$("#receiptnum").val("")
                        ordernumber = ""; 
                        //var arrayLocation = new Array();
                        //var arraySKU = new Array();
                        objtotal.length = 0; 
                        CloseDiv();
                        $("#ordernum").focus();
                        return;
                    }
                    else {
                        alert("提交包装失败！");
                    }

                },
                error: function (msg) {
                    alert("网络连接失败！");
                }

            });
        
    }
