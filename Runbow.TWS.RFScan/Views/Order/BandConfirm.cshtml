    function Scan(event) {
        if (event.keyCode == 13) {
            if ($("#Scan").val() == "") {
                alert("扫描数据不能为空！");
                return;
            }
            var scanval = $("#Scan").val();
            if ($("#ordernum").val() == "") {
                alert("请先扫描出库单！");
                return;
            }
            $("#expressnum").val(scanval);
            //保存扫描的快递单
            saveexpressnum();
            if (SelectIndexadd()) {
                if (confirm("是否提交包装信息？")) {
                    PreSaveRecdata();
                }
                else {
                    $("#Scan").focus();
                    $("#Scan").select();
                }
            }
        }
    }
    //跳转到下拉框
    function SelectIndexadd() {
        var isOver = true;
        var select = document.getElementById("boxno");
        var lastIndex = select.selectedIndex;
        if (lastIndex < select.length-1) {
            select.value = select.options[lastIndex+1].value;
            isOver = false;
        }
        ScanFouce($("#boxno"));
        return isOver;
      
    }
