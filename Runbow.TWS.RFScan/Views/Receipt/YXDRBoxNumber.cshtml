    //验证数据是否是库位
    function checkLocation(location) {
        if ($("#ck_scan").get(0).checked && $("#Scan").val().length >= 2 && $("#Scan").val().substring(0, 2) == "00") {
            location = location.substring(2, location.length);
        }
        var result = false;
        $.ajax({
            type: "Post",
            url: '/Receipt/CheckLocation',
            data: { "Location": location },
            async: false,
            success: function (data) {
                if (data != "") {
                    AreaName = data;//保存当前库区
                    //location.href = "/WMS/ReceiptManagement/ReceiptCreate/?ID=" + ID + "&ViewType=3&Flag=1"
                    result = true;
                    $("#Scan").val(location);
                }
                else {
                    result = false;
                }

            },
            error: function (msg) {
                result = false;
            }

        });

        return result;
    }
