        //验证数据是否是箱号
        function checkBoxNumber(ReceiptNumber, BoxNumber) {
            var result = false;
            $.ajax({
                type: "Post",
                url: '/Receipt/CheckBoxNumberBack',
                data: { "ReceiptNumber": ReceiptNumber, "BoxNumber": BoxNumber },
                async: false,
                dataType: "json",
                success: function (data) {
                    if (data.Code == "1") {
                        result = true;                                              
                    }
                    else if (data.Code == "2") {
                        result = true;
                        if (data.data.Location != "") {
                            $("#location").val(data.data.Location.split(',')[0]);
                            $("#locationlabel")[0].innerText = "已放数量：" + data.data.Location.split(',')[1];
                        }
                        if (data.data.LocationS != "") {
                            $("#locationS").val(data.data.LocationS);
                        }
                        
                    }
                    else {
                        result = false;
                    }

                },
                error: function (msg) {
                    result = false;
                }

            });

            return result;
        }

