        </tr>
        <tr style="height:10px">
            <td>入库单号</td>

            <td><input style="width:250px" id="receiptnum" value="@ViewBag.ReceiptaNumber" disabled="disabled" type="text" onkeydown="checkRecNum(event)" /></td>
        </tr>
        <tr>
            <td colspan="2">
                <div style="width:140px;float:left">产品已扫/产品总数</div>
                <div style="width:140px;float:left;margin-left:30px">本箱已扫/本箱总数</div>
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <div style="width:140px;float:left">
                    <input style="width:60px;" id="skuqty" type="text" onkeydown="ScanSKUQTY(event)" disabled="disabled" />
                    <input style="width:60px" id="skuqtyAll" type="text" onkeydown="ScanSKUQTYAll(event)" disabled="disabled" />
                </div>
                <div style="width:140px;float:left;margin-left:30px">
                    <input style="width:60px;color:white" id="qty" type="text" onkeydown="ScanQTY(event)" disabled="disabled" />
                    <input style="width:60px" id="qtyAll" type="text" onkeydown="ScanQTYAll(event)" disabled="disabled" />
                    </div>
            </td>
        </tr>
        <tr>
            <td>扫&nbsp;&nbsp;描&nbsp;&nbsp;区</td>
            <td><input style="width:250px;border-color:#4cff00" id="Scan" type="text" onkeydown="Scan(event)" onfocus="ScanFocus()" onblur="ScanBlur()"/></td>
        </tr>
        <tr>
            <td colspan="2" style="text-align:center">
                <input id="show" style="width:90px" onclick="showzjmsg()" type="button" value="查看已上库位">
                <input id="showdiff" style="width:90px" onclick="showdiffmsg()" type="button" value="查看箱差异">
                <input id="showReceiptdiff" style="width:90px" onclick="showReceiptdiffmsg()" type="button" value="查看单差异">
                @if (ViewBag.UserName == "shthrfadmin")
                {
                <input id="ScanAgainBox" style="width:90px;background-color:red" onclick="ScanAgainBoxNumber()" type="button" value="本箱重扫">
                }

            </td>
        </tr>
    </table>
    <div id="showdata" style="position:absolute;top:0px;left:0px;width:400px;height:300px;display:none;background-color:white;border:solid 1px #add9c0;overflow:scroll;">
        <table id="showtable" style="width:410px"></table>
        <div style="text-align:center"><input style="width:50%" type="button" value="关闭" onclick="CloseDiv()" /></div>
    </div>
    <div id="showdiffdata" style="display:none;width:320px">
        <table id="checkdifftable" style="width:320px">
            <thead>
                <tr style="text-align:left">
                    <th style="text-align:left">SKU</th>
                    <th style="text-align:left">差异数量</th>
                </tr>
            </thead>
            <tbody style="text-align:left"></tbody>
        </table>
    </div>


    <div id="showreceiptdiffdata" style="display:none;width:400px">
        <table id="checkreceiptdifftable" style="width:430px">
            <thead>
                <tr>
                    <th>箱号</th>
                    <th>SKU</th>
                    <th>差异数量</th>
                </tr>
            </thead>
            <tbody style="text-align:center"></tbody>
        </table>
    </div>

    <div id="showredistaskdata" style="display:none;width:320px">
        <table>
            <tr>
                <td>
                    <input style="width:300px; border: 1px solid #428BCA; border-radius: 7px; outline: none;background: none; padding: 5px 0;" id="skuagain" onkeydown="ScanAgain(event)" autofocus type="text" />
                </td>

            </tr>
            <tr>
                <td><label id="boxNumberLb"></label></td>
            </tr>
        </table>
        <table id="checkredistasktable" style="width:320px">
            <thead>
                <tr style="text-align:left">
                    <th style="text-align:left">SKU</th>
                    <th style="text-align:left">数量</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

    <div id="showzjdata" style="display:none;width:400px">
        <table id="checkzjtable" style="width:450px">
            <thead>
                <tr style="text-align:left">
                    <th style="text-align:left">箱号</th>
                    <th style="text-align:left">SKU</th>
                    <th style="text-align:left">数量</th>
                    <th style="text-align:left">库位</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

</body>
</html>