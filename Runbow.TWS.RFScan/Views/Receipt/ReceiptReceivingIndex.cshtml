    <script src="~/Assets/JS/jquery-1.11.3.min.js"></script>
    <script src="~/Assets/JS/Layer/layer.js"></script>
    @model Runbow.TWS.RFScan.Models.ReceiptViewModel
    <title>退货上架</title>
    <script type="text/javascript">
        $(document).ready(function () {
            pushHistory();
            window.addEventListener("popstate", function (e) {
                if (lastSKUobj.length > 0) {
                    //layer.msg("请扫描库位保存当前数据");
                    alert("请扫描库位保存当前数据");
                    pushHistory();
                }
                //根据自己的需求实现自己的功能
                //window.location.href = 'https://www.baidu.com'
            }, false);
            window.addEventListener("beforeunload", listenClose, false);
            $("#Scan").focus();
            $("#upc").attr("readonly", "readonly");
            $("#sku").attr("readonly", "readonly");
            $("#location").attr("readonly", "readonly");
            checkRedisTask();//查询是否有异常退出任务
            checkRecNumScan();

        });
