@model Runbow.TWS.LogisticsReport.Models.MonitoringReport
<link rel="stylesheet" href="~/Assets/bootstrap-3.3.4-dist/bootstrap.min.css">
<link href="~/Assets/bootstrap-3.3.4-dist/bootstrap-theme.min.css" rel="stylesheet" />
<script src="~/JS/jquery-1.8.3.min.js"></script>
<script src="~/JS/jquery-ui-1.8.20.min.js"></script>
<script src="~/JS/jquery.datetimepicker.js"></script>
<link href="~/CSS/jquery-ui.css" rel="stylesheet" />
<link href="~/CSS/jquery.ui.datepicker.css" rel="stylesheet" />
<link href="~/CSS/jquery.ui.all.css" rel="stylesheet" />
<script src="~/JS/Home/AddElectricNumerical.js"></script>
<style type="text/css">
    #centerdiv {
        position: absolute;
        top: 50%;
        left: 50%;
        margin: -150px 0 0 -100px;
        /*width: 300px;*/
        /*height: 200px;*/
        /*background: black;*/
    }
</style>
<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        $(".datepicker").datepicker({
            numberOfMonths: 1,
            showButtonPanel: true,
            changeMonth: true,
            changeYear: true,
            "yearRange": '1950:2020',
            showMonthAfterYear: true
        });
    });
</script>
<body>
    <div id="centerdiv">
        <table>
            <thead>
            </thead>
            <tbody>


                <tr>
                    <td>
                        Time
                    </td>
                    <td>
                        <input readonly="true" id="ThisTime" class="datepicker form-control" value="@System.DateTime.Now.ToString("MM/dd/yyyy")" data-date-format="yyyy/MM/dd" placeholder="starttime" data-date-autoclose="true" data-date-disabletouchkeyboard="true" data-date-todayhighlight="true" data-date-showonfocus="false">
                    </td>
                </tr>
                <tr>
                    <td>
                        Office
                    </td>
                    <td>
                        <input type="text" id="Office" class="form-control " value="" />
                    </td>
                </tr>

                <tr>
                    <td>
                        NFS
                    </td>
                    <td>
                        <input id="NFS" class="  form-control" value="">
                    </td>
                </tr>
                <tr>
                    <td>
                        Digital
                    </td>
                    <td>
                        <input type="text" id="Digital" class="form-control " value="" />
                    </td>
                </tr>
                <tr>
                    <td>
                        Inline
                    </td>
                    <td>
                        <input id="Inline" class="  form-control" value="">
                    </td>
                </tr>
            </tbody>
        </table>

        <div class="actionButtonDiv" style="margin-left:25%;padding-top:20px;">
            <button class="btn btn-success" id="Oksubmit">Submit</button>
            <button class="btn btn-success" id="Goreport">Return</button>
        </div>
    </div>
</body>
